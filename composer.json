{
    "name": "trendyminds/craft-spacing",
    "description": "A utility field to insert spacing between your Matrix blocks",
    "type": "craft-plugin",
    "version": "2.0.0",
    "license": "mit",
    "support": {
        "email": "dev@trendyminds.com",
        "issues": "https://github.com/trendyminds/craft-spacing/issues?state=open",
        "source": "https://github.com/trendyminds/craft-spacing",
        "docs": "https://github.com/trendyminds/craft-spacing",
        "rss": "https://github.com/trendyminds/craft-spacing/releases.atom"
    },
    "require": {
        "php": "^8.2",
        "craftcms/cms": "^5.0.0-alpha.3"
    },
    "autoload": {
        "psr-4": {
            "trendyminds\\spacing\\": "src/"
        }
    },
    "extra": {
        "handle": "spacing",
        "name": "Spacing",
        "developer": "TrendyMinds",
        "documentationUrl": "https://github.com/trendyminds/craft-spacing",
        "class": "trendyminds\\spacing\\Spacing"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "require-dev": {
        "laravel/pint": "^1.5"
    }
}
